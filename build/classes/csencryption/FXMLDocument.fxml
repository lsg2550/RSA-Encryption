      <Button fx:id="testPrime" layoutX="357.0" layoutY="46.0" mnemonicParsing="false" onAction="#gen_pq" text="Test Prime" />
      <TextField fx:id="message" layoutX="187.0" layoutY="373.0" prefHeight="25.0" prefWidth="160.0" />
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="187.0" layoutY="349.0" prefHeight="17.0" prefWidth="160.0" text="Message to Encrypt/Decrypt" textAlignment="CENTER" />
      <Button fx:id="clear" layoutX="479.0" layoutY="549.0" mnemonicParsing="false" onAction="#clear" text="Clear Text" />
      <Button fx:id="genKey1" layoutX="301.0" layoutY="300.0" onAction="#generateKEYS" text="Generate Keys" visible="false" />
      <TextField fx:id="userE" layoutX="14.0" layoutY="169.0" prefHeight="25.0" prefWidth="72.0" visible="false" />
      <TextField fx:id="userD" layoutX="86.0" layoutY="169.0" prefHeight="25.0" prefWidth="72.0" visible="false" />
