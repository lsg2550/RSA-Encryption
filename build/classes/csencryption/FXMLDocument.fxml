<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="csencryption.FXMLDocumentController">
    <children>
        <Button fx:id="genKey" layoutX="393.0" layoutY="300.0" onAction="#genAutoKeys" text="Auto-Gen Keys" visible="false" />
      <TextField fx:id="pm1" layoutX="198.0" layoutY="46.0" promptText="Enter a Prime Number" />
      <TextField fx:id="pm2" layoutX="439.0" layoutY="46.0" promptText="Enter a Prime Number" />
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="198.0" layoutY="29.0" prefHeight="17.0" prefWidth="149.0" text="Prime Number 1 (p)" />
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="439.0" layoutY="29.0" prefHeight="17.0" prefWidth="149.0" text="Prime Number 2 (q)" />
      <TextArea fx:id="puKey" layoutX="173.0" layoutY="88.0" prefHeight="200.0" prefWidth="200.0" />
      <TextArea fx:id="prKey" layoutX="414.0" layoutY="88.0" prefHeight="200.0" prefWidth="200.0" />
      <Label alignment="CENTER" layoutX="174.0" layoutY="71.0" prefHeight="17.0" prefWidth="200.0" text="Public Key" />
