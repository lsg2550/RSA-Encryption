      <Label fx:id="labelD" layoutX="119.0" layoutY="152.0" text="d" visible="false" />
      <Button fx:id="testRP" layoutX="24.0" layoutY="200.0" mnemonicParsing="false" onAction="#testRP" text="Test Relatively Prime" visible="false" />
